    <stylesheets>
<!--        <URL value="@/styles/main/passwordRecoveryStage2.css" />-->
    </stylesheets>
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="167.0" layoutY="85.0" style="-fx-text-fill: #1a376d;" text="Odzyskiwanie hasła">
